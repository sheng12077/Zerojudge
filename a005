n = int(input())
for i in range(1, n+1):
    tmp = input()
    tmp2 =tmp.strip()
    a, b, c, d = tmp2.split()
    a = int(a)
    b = int(b)
    c = int(c)
    d = int(d)
    if b-a == c-b:
        e = d+b-a
        print(a, b, c, d, e)
    elif b**2 == a*c:
        e = int(d*b/a)
        print(a, b, c, d, e)
#a005
